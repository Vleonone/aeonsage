{
  "name": "aeonsage-colab",
  "version": "1.0.0",
  "description": "AeonSage Colab Environment Configuration",
  "private": true,
  "scripts": {
    "colab:setup": "bash ../colab/quick_setup.sh",
    "colab:start": "pnpm aeonsage gateway run --bind 0.0.0.0 --port 18789",
    "colab:test": "pnpm test -- --run",
    "colab:dev": "AEONSAGE_PROFILE=colab pnpm dev",
    "colab:doctor": "pnpm aeonsage doctor"
  },
  "dependencies": {
    "aeonsage": "file:.."
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "pnpm": {
    "overrides": {
      "tar": "7.5.7"
    }
  }
}